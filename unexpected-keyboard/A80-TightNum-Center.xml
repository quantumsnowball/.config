<?xml version="1.0" encoding="utf-8"?>
<keyboard name="A80-TightNum-Center" script="latin" bottom_row="false">
  <row>
    <key c="1" se="!" sw="`" nw="~" />
    <key c="2" se="@"/>
    <key c="3" se="#"/>
    <key c="4" se="$"/>
    <key c="5" se="%"/>
    <key c="6" sw="^"/>
    <key c="7" sw="&amp;"/>
    <key c="8" sw="*"/>
    <key c="9" sw="(" se="-" ne="_"/>
    <key c="0" sw=")" se="=" ne="+"/>
  </row>
  <row>
    <key c="q" />
    <key c="w" />
    <key c="e" />
    <key c="r" s="redo" />
    <key c="t" s="paste"/>
    <key c="y" s="copy" />
    <key c="u" s="undo" />
    <key c="i" se="[" ne="{" />
    <key c="o" se="]" ne="}" />
    <key c="p" se="\\" ne="|" s="pasteAsPlainText" />
  </row>
  <row>
    <key c="a" s="selectAll" shift="0.5" />
    <key c="s" />
    <key c="d" />
    <key c="f" />
    <key c="g" s="selectAll"/>
    <key c="h" />
    <key c="j" />
    <key c="k" se=";" ne=":" />
    <key c="l" se="'" ne="&quot;" />
  </row>
  <row>
    <key c="esc" n="shift" e="tab" width="1.5" />
    <key c="z" s="switch_clipboard" />
    <key c="x" s="cut" />
    <key c="c" s="copy" />
    <key c="v" s="paste" />
    <key c="b" se="," ne="&lt;" />
    <key c="n" se="." ne="&gt;" />
    <key c="m" se="/" ne="\?" />
    <key c="backspace" n="shift" s="delete" width="1.5" />
  </row>
  <row height="0.95">
    <key c="fn" 
      n="config"
      s="switch_forward"
      width="1.1" 
    />
    <key c="ctrl" 
      w="alt"
      e="tab"
      width="1.7" 
    />
    <key c="space" 
      n="up"
      s="down"
      e="right"
      w="left"
      width="4.4" 
    />
    <key c="." 
      n="page_up"
      s="page_down"
      e="end"
      w="home"
      width="1.1" 
    />
    <key c="enter" 
      n="ctrl"
      width="1.7" 
    />
  </row>
</keyboard>
