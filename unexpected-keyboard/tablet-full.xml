<?xml version="1.0" encoding="utf-8"?>
<keyboard name="tablet-full" script="latin" bottom_row="false">
  <row>
    <key c="tab" sw="`" nw="~"/>
    <key c="q" n="1" nw="!" />
    <key c="w" n="2" nw="@" />
    <key c="e" n="3" nw="#" />
    <key c="r" n="4" nw="$" se="redo" />
    <key c="t" n="5" nw="%" />
    <key c="y" n="6" sw="copy" />
    <key c="u" n="7" sw="undo" />
    <key c="i" n="8" se="[" ne="{" />
    <key c="o" n="9" se="]" ne="}" />
    <key c="p" n="0" se="\\" ne="|" sw="pasteAsPlainText" />
    <key c="backspace" />
  </row>
  <row>
    <key c="esc" />
    <key c="a" se="selectAll" />
    <key c="s" />
    <key c="d" />
    <key c="f" />
    <key c="g" />
    <key c="h" />
    <key c="j" />
    <key c="k" />
    <key c="l" />
    <key c=";" sw=":" />
    <key c="'" sw="&quot;" />
  </row>
  <row>
    <key c="shift" />
    <key c="z" s="switch_clipboard" />
    <key c="x" s="cut" />
    <key c="c" s="copy" />
    <key c="v" s="paste" />
    <key c="b"  />
    <key c="n"  />
    <key c="m" />
    <key c="," sw="&lt;"/>
    <key c="." sw="&gt;"/>
    <key c="/" sw="\?"  />
    <key />
  </row>
  <row height="0.95">
    <key c="fn" 
      n="config"
      s="switch_forward"
      width="1.1" 
    />
    <key c="ctrl" 
      w="alt"
      e="tab"
      width="1.7" 
    />
    <key c="space" 
      n="up"
      s="down"
      e="right"
      w="left"
      width="6.4" 
    />
    <key c="." 
      n="page_up"
      s="page_down"
      e="end"
      w="home"
      width="1.1" 
    />
    <key c="enter" 
      n="ctrl"
      width="1.7" 
    />
  </row>
</keyboard>

